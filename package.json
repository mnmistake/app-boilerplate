{
  "name": "kyrene",
  "version": "1.0.0",
  "description": "GraphQL based API written in Golang, backed by PostgreSQL",
  "author": "Rauno Freiberg",
  "license": "MIT",
  "homepage": "https://github.com/raunofreiberg/kyrene#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raunofreiberg/kyrene.git"
  },
  "bugs": {
    "url": "https://github.com/raunofreiberg/kyrene/issues"
  },
  "scripts": {
    "client": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host=0.0.0.0 --inline --progress --colors",
    "server": "go run main.go",
    "build": "webpack -p --config webpack/webpack.prod.js --progress --profile --color",
    "prod": "concurrently \"NODE_ENV=production npm run build\" \"ENV=production npm run server\"",
    "lint": "eslint client/ --ext .js --ext .jsx",
    "test": "NODE_ENV=test jest --forceExit",
    "cover": "istanbul cover npm test --no-default-excludes",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-client-preset": "^1.0.6",
    "apollo-link-context": "^1.0.5",
    "apollo-link-http": "^1.3.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-loader": "^2.2.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.3",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.7.3",
    "history": "^4.7.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.1.1",
    "resolve-url-loader": "^2.0.3",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "^2.4.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1"
  }
}
